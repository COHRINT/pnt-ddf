[ENV]
agent_names = ["A", "B", "C", "D", "T"]
transmission_window = 0.1
max_time = 100
logging = False

[FILTER]
alpha = 1.0e0
beta = 2.0
q_sigma_clock = 1
q_sigma_target = 1

[x0]
b = 0
b_dot = 0
x = 0
y = 0
x_dot = 0.0
y_dot = 0.0

[P0]
sigma_b = 1e1
sigma_b_dot = 1e-1
sigma_x = 1e3
sigma_y = 1e3
sigma_x_dot = 1e-1
sigma_y_dot = 1e-1

[T]
x_0 = 0
y_0 = 0
x_dot_0 = 0.0
y_dot_0 = 0.0
estimate_clock = False
perfect_clock = True
alpha = 1.0
beta = 0.0
wait = 0
sigma_clock_process = 51e-9
sigma_clock_reading = 0.13e-9
odometer = True
sigma_odometer = 1e-1

[A]
